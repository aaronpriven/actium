#!/bin/perlopen IN, "6-wb.html";# not a sophisticated open routine, is it?do {   $_ = <IN>;} until /<pre>/;# skip everything until "pre"$_ = <IN>;until (m@</pre>@) {# keep reading data until the close of the <pre>   if (/<hr>/)      # hr signals a block of repeated timepoint info, which we don't   # need   {       $_ = <IN>;       $_ = <IN>;       next;   }   chomp;   push @sched, $_ if $_;   # add it to the array, unless it's blank   $_ = <IN>;}# @sched is now scheduleprint join "\n", @sched;print "\n";
#!/usr/bin/perl5$linenum = $ARGV[0];$infile =     'lynx -source "http://www.transitinfo.org/cgi-bin/sched?C=AC&R='    . $linenum . '&E=New" |';open IN, $infile;# so that gets the schedule markerwhile (<IN>) {   push @scheds, $_ if /href=\"sched?.*DR=/}foreach (@scheds) {   chomp;   s/.*?href="//;   ($url, $_) = split ('"',$_,2)      s@^">@@;   s@</a>$@@;   $url{$_} = $url;}